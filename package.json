{
  "name": "dcs-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:acc": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --configuration=acc",
    "serve:l12": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --configuration=l12",
    "serve:acc-dev": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --configuration=acc-dev",
    "serve:l12-dev": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --configuration=l12-dev",
    "build": "ng build",
    "build:acc": "ng build --configuration=acc",
    "build:l12": "ng build --configuration=l12",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.4",
    "@angular/cdk": "~18.0.4",
    "@angular/cli": "^18.1.0",
    "@angular/common": "^18.0.4",
    "@angular/compiler": "^18.0.4",
    "@angular/core": "^18.0.4",
    "@angular/forms": "^18.0.4",
    "@angular/material": "^18.0.4",
    "@angular/platform-browser": "^18.0.4",
    "@angular/platform-browser-dynamic": "^18.0.4",
    "@angular/router": "^18.0.4",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@ng-idle/core": "^14.0.0",
    "@ng-idle/keepalive": "^14.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "angular-gridster2": "^18.0.1",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.4.1",
    "dayjs": "^1.11.10",
    "html2canvas": "^1.4.1",
    "lodash": "^4.17.21",
    "ng-table-virtual-scroll": "^1.6.0",
    "ngx-echarts": "^17.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.5",
    "@angular/compiler-cli": "^18.0.4",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.14.202",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.5"
  }
}
